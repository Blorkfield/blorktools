{
  "name": "@blorkfield/asset-debugger",
  "version": "0.4.0",
  "description": "3D asset debugging and visualization tool",
  "type": "module",
  "private": false,
  "main": "./bin/cli.js",
  "bin": {
    "asset-debugger": "./bin/cli.js"
  },
  "files": [
    "bin",
    "axis-indicator",
    "debugger-scene",
    "header",
    "landing-page",
    "loading-splash",
    "modals",
    "panels",
    "util",
    "widgets",
    "index.html",
    "main.css",
    "router.js",
    "favicon.ico"
